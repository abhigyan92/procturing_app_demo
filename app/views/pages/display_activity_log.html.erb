
<div class="d-flex align-items-center flex-column">
<%= link_to 'Download recording', @recording.video.url %>
<h2>Activities</h2>
<label class="text-info"><%= @user.email %> </label>
<table class="table table-striped w-75">
  <tr>
    <th>Event Name</th>
    <th>Value</th>
    <th>Time</th>
  </tr>
  <% @submissions.each do |s| %>
    <tr>
      <td colspan="3" class="text-center">Test Submitted at <%= s.updated_at %></td>
    </tr>
    <% s.procturing_events.each do |pe| %>
      <tr>
        <td><%= pe.name %></td>
        <td><%= pe.value != 'undefined' ? pe.value : '--' %></td>
        <td><%= pe.created_at %></td>
      </tr>
    <% end %>
  <% end %>
</table>
