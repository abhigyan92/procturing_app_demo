<h2><%= @user.email %> Activities</h2>
<%= link_to 'Download recording', @recording.video.url %>

<table>

  <tr>
    <th>Event Name</th>
    <th>Value</th>
    <th>Time</th>
  </tr>
  <% @procturing_events.each do |pe| %>
    <tr>
      <td><%= pe.name %></td>
      <td><%= pe.value %></td>
      <td><%= pe.created_at %></td>
    </tr>
  <% end %>

</table>
